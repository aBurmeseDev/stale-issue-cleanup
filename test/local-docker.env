# This environment file is used for local testing with Docker.

# A personal access token with repo permissions
REPO_TOKEN=

ANCIENT_ISSUE_MESSAGE=Ancient issue message
STALE_ISSUE_MESSAGE=Stale issue message
STALE_PR_MESSAGE=Stale pr message
DAYS_BEFORE_STALE=7
DAYS_BEFORE_CLOSE=4
DAYS_BEFORE_ANCIENT=1095
STALE_ISSUE_LABEL=closing-soon
EXEMPT_ISSUE_LABEL=automation-exempt
STALE_PR_LABEL=closing-soon
EXEMPT_PR_LABEL=automation-exempt
RESPONSE_REQUESTED_LABEL=response-requested
MINIMUM_UPVOTES_TO_EXEMPT=1
DRYRUN=true
LOGLEVEL=DEBUG

# Change this to the repo you're testing
GITHUB_REPOSITORY=octocat/Hello-World
GITHUB_REPOSITORY_OWNER=octocat

GITHUB_ACTIONS=true
CI=true
